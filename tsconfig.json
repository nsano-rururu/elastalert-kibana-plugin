{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2015", 
    "module": "commonjs",
    "lib": [ "es2015"],
    "esModuleInterop": true,
    "outDir": "./target",
    "skipLibCheck": true
